{
  "expo": {
    "name": "Sleepr",
    "slug": "sleeprapp",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcons/ios.xcassets/1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "38",
      "bundleIdentifier": "com.sleeprapp.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcons/android/mipmap-xxxhdpi/ic_launcher.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sleeprapp.app",
      "versionCode": "1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/AppIcons/ios.xcassets/1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cf59327a-0951-40cb-b51b-f3dc032eab40"
      }
    },
    "owner": "donaldlee50"
  }
}
